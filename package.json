{
  "name": "mjolnir.js",
  "description": "An Event Manager",
  "version": "1.2.1",
  "keywords": [
    "hammerjs",
    "touch",
    "gesture",
    "mouse",
    "keyboard",
    "event manager"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/uber-web/mjolnir.js"
  },
  "license": "MIT",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "files": [
    "src",
    "dist",
    "dist-es6"
  ],
  "scripts": {
    "start": "(cd examples/main && (path-exists node_modules || npm i) && npm run start-local)",
    "clean": "rm -fr dist",
    "build": "npm run clean && rollup -c",
    "lint": "eslint src examples test",
    "test": "npm run lint && node test/node.js",
    "publish-prod": "npm run build && npm run test && npm publish",
    "publish-beta": "npm run build && npm run test && npm publish --tag beta"
  },
  "pre-commit": [
    "test"
  ],
  "dependencies": {
    "hammerjs": "^2.0.8"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-eslint": "^6.0.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.18",
    "eslint": "^3.0.0",
    "eslint-config-uber-es2015": "^3.0.0",
    "eslint-config-uber-jsx": "^3.0.0",
    "eslint-plugin-react": "~6.7.0",
    "module-alias": "^2.0.0",
    "pre-commit": "^1.2.2",
    "reify": "^0.4.4",
    "rollup": "^0.64.1",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-size-snapshot": "^0.6.1",
    "tap-browser-color": "^0.1.2",
    "tape": "^4.5.1",
    "tape-catch": "^1.0.4",
    "webpack": "^2.4.0",
    "webpack-dev-server": "^2.4.0"
  },
  "engines": {
    "node": ">= 4",
    "npm": ">= 3"
  }
}
